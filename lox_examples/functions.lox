fun sayHi(first, last) {
  print "Hi, " + first + " " + last + "!";
}

sayHi("Dear", "Reader");

print "The first twenty fibonacci numbers";

fun fib(n) {
    if (n <= 1) return n;
    return fib(n - 2) + fib(n - 1);
}

var startTime = clock();
for (var i=0; i< 20 ; i = i + 1) {
    print fib(i);
}
var timeSpent = clock() - startTime;
print "Time spent: " + timeSpent;